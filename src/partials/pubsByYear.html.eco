      <% @pubsByYear = @groupBy(@pubs, (p) => if p['year'] then p['year'] else p['date'].substring(0,4)) %>
      <% @years = Object.keys(@pubsByYear).map((x) => parseInt(x)).sort().reverse() %>
      <% for year in @years: %>
        <div class="year">
          <h3><%= year %></h3>
          <%- @partial('pubs', false, { pubs: @pubsByYear[year], people: @people }) %>
        </div>
      <% end %>    
